	bool	UPD_FLG;			//更新有りフラグ
	bool	SAVE_FLG;			//保存フラグ
    char    UPDNAME[50];        //表示用更新者ネーム	//2003.05.17 E.Takase Add

//    char  	HINBAN[9];         	//工管発行日
    char  	KOKBNG[7];         	//工管番号
};

