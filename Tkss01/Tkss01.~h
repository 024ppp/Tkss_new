    char	LOTNOCHANGE6_SZ[2]; //素材検査用ロット変更フラグNO6
    char  	HINBAN[21];         //自社品番
    char  	DTKSCOD[6];         //得意先コード
    char  	DTKSNAM[51];        //得意先名称
    char  	ZISNAM[21];         //材質名称
    char  	HINMEI[61];         //品名
    char  	DTKSHIN1_SZ[21];    //素材用得意先品番１
