    char  	DISP_VAL02[15];     //印刷用測定値２
    char  	DISP_VAL03[15];     //印刷用測定値３
    char  	DISP_VAL04[15];     //印刷用測定値４
    char  	DISP_VAL05[15];     //印刷用測定値５
