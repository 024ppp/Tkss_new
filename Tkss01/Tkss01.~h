    char    OUT_TITLE2[5];     	//成績所タイトル出力項目２
    char    OUT_TITLE3[5];     	//成績所タイトル出力項目３
    char    OUT_TITLE4[5];     	//成績所タイトル出力項目４
    char    OUT_TITLE5[5];     	//成績所タイトル出力項目５
    char    OUT_TITLE6[5];     	//成績所タイトル出力項目６
    char   	OUT_TITLE7[5];     	//成績所タイトル出力項目７
    char   	OUT_TITLE8[5];     	//成績所タイトル出力項目８
    char   	X_OUT[2];          	//Xバーの出力
    char  	MEMO[129];        	//備考
    char  	SKTYMD_SZ[9];       //素材用測定日
    char  	SKTYMD_SZ_PRT[9];   //素材用測定日（印刷用）
    char  	SKTYMD[9];         	//出荷用測定日
    char  	SKTYMD_PRT[9];      //出荷用測定日（印刷用）
    char  	ADDYMD[9];         	//代表登録日
    char  	ADDTIM[5];         	//代表登録時間
    char  	UPDYMD[9];         	//代表変更日
    char  	UPDTIM[5];         	//代表変更時間
    char    UPDCHR[7];         	//代表更新者
    char    UPDCNT[4];         	//代表更新回数

    char  	KOMKNO1_SZ[5];    	//素材用得意先品番１
    char  	KOMKNO2_SZ[5];    	//素材用得意先品番２
    char  	KOMKNO3_SZ[5];    	//素材用得意先品番３
    char  	KOMKNO4_SZ[5];    	//素材用得意先品番４
    char  	KOMKNO5_SZ[5];    	//素材用得意先品番５
    char  	KOMKNO6_SZ[5];    	//素材用得意先品番６
    char  	KOMKNM1_SZ[41];    	//素材用得意先品番１
    char  	KOMKNM2_SZ[41];    	//素材用得意先品番２
    char  	KOMKNM3_SZ[41];    	//素材用得意先品番３
    char  	KOMKNM4_SZ[41];    	//素材用得意先品番４
    char  	KOMKNM5_SZ[41];    	//素材用得意先品番５
    char  	KOMKNM6_SZ[41];    	//素材用得意先品番６
    char  	KOMKSUB1_SZ[21];    //素材用得意先品番１
    char  	KOMKSUB2_SZ[21];    //素材用得意先品番２
    char  	KOMKSUB3_SZ[21];    //素材用得意先品番３
    char  	KOMKSUB4_SZ[21];    //素材用得意先品番４
    char  	KOMKSUB5_SZ[21];    //素材用得意先品番５
