struct KSD01_DATA {          	//測定実績データ
    char  	DTKSHIN[21];        //得意先品番
    char  	LOTNO[7];           //ロットNo
    char	LOTNO1_SZ[7];	    //素材検査用ロットNo1
    char 	LOTNO2_SZ[7];       //素材検査用ロットNO2
    char 	LOTNO3_SZ[7];       //素材検査用ロットNO3
    char 	LOTNO4_SZ[7];       //素材検査用ロットNO4
    char 	LOTNO5_SZ[7];       //素材検査用ロットNO5
    char 	LOTNO6_SZ[7];       //素材検査用ロットNO6
    char	LOTNOCHANGE1_SZ[2]; //素材検査用ロット変更フラグNO1
    char	LOTNOCHANGE2_SZ[2]; //素材検査用ロット変更フラグNO2
